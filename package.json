{
  "name": "mem-fs",
  "version": "0.0.1",
  "description": "In memory filesystem",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist/ coverage/ .nyc_output/ node_modules/.cache/",
    "lint": "standard {src,test}/*.js | snazzy",
    "format": "prettier-standard {src,test}/*.js",
    "test": "nyc ava test/*.js",
    "coverage": "npm run test && nyc report --reporter=html && http-server coverage/ -p 8000",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "/nas/config/git/mem-fs.git"
  },
  "author": "",
  "license": "Unlicense",
  "devDependencies": {
    "ava": "^1.4.1",
    "esm": "^3.2.22",
    "http-server": "^0.11.1",
    "nyc": "^13.3.0",
    "prettier-standard": "^9.1.1",
    "rollup": "^1.8.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "dependencies": {
    "ow": "^0.12.0"
  }
}
